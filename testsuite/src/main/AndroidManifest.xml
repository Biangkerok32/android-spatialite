<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.spatialite.testsuite">

    <application
        android:name="org.spatialite.testsuite.SpatialiteApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <provider
            android:name="org.spatialite.testsuite.SpatialiteContentProvider"
            android:authorities="org.spatialite.testsuite.provider"/>
        <provider
            android:name="org.spatialite.testsuite.SpatialiteContentProvider2"
            android:authorities="org.spatialite.testsuite.provider2"
            android:process=":provider"/>

        <activity
            android:name="org.spatialite.testsuite.TestSuiteActivity"
            android:icon="@drawable/icon"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
    </application>

</manifest>
